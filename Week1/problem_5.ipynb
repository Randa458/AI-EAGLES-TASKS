    "def sum_of_digits(n):\n",
    "    return sum(int(digit) for digit in str(n))\n",
    "\n",
    "number = int(input())\n",
    "counter = 0\n",
    "\n",
    "while number >= 10:\n",
    "    number = sum_of_digits(number)\n",
    "    counter += 1\n",
    "\n",
    "print(counter)\n"
   ]
